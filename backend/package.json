{
  "name": "backend",
  "version": "0.1.0",
  "bin": {
    "backend": "bin/backend.js"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "build:watch": "tsc -w",
    "clean": "rm -rf dist && find . -name '*.js' -not -name 'jest.config.js' -not -path './node_modules/*' -delete && find . -name '*.d.ts' -not -path './node_modules/*' -delete",
    "test": "jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "2.1029.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.153",
    "@types/pg": "^8.15.5",
    "aws-cdk-lib": "^2.215.0",
    "aws-sdk": "^2.1692.0",
    "constructs": "^10.4.2",
    "pg": "^8.16.3"
  }
}
